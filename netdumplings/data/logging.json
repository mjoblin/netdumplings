{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "standard": {
            "format": "%(asctime)s.%(msecs).03d.UTC %(levelname)s %(message)s",
            "datefmt": "%Y-%m-%d.%H:%M:%S"
        }
    },
    "handlers": {
        "default": {
            "formatter": "standard",
            "class": "logging.StreamHandler"
        }
    },
    "loggers": {
        "netdumplings": {
            "handlers": ["default"],
            "level": "INFO",
            "propagate": false
        },
        "netdumplings.dumplinghub": {
            "handlers": ["default"],
            "level": "INFO",
            "propagate": false
        },
        "netdumplings.dumplingkitchen": {
            "handlers": ["default"],
            "level": "INFO",
            "propagate": false
        },
        "netdumplings.dumplingeater": {
            "handlers": ["default"],
            "level": "INFO",
            "propagate": false
        }
    }
}
